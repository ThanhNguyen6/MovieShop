@model List<ApplicationCore.Models.Response.MovieResponseModel>
@{
    ViewData["Title"] = "Home Page";
}

<div>
    MovieShop Home Page
</div>

@foreach (var movie in Model)
            {
                <ul>
                    <li>@movie.Id</li>
                    <li>@movie.Title</li>
                    <li>@movie.Budget</li>
                </ul>

            }
